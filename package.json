{
  "name": "unlimited-blog-works",
  "version": "0.0.6",
  "description": "A simple static blog generator for publishing on GitHub Pages",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build": "tsc --project ./tsconfigs/build.json",
    "build-and-publish": "rm -rf ./dist && npm run build && npm publish",
    "serve:docs": "npm run serve:docs:prepare && npm run serve:docs:run",
    "serve:docs:prepare": "rm -rf ./test-serving && mkdir test-serving && cp -r docs test-serving/unlimited-blog-works",
    "serve:docs:run": "serve test-serving",
    "test": "mocha --require './setup/ts-node-reigister-for-test.js' './test/**/*-test.ts'",
    "tsc": "tsc --project ./tsconfigs/test.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjirou/unlimited-blog-works.git"
  },
  "keywords": [
    "blog",
    "static-site-generator",
    "tool"
  ],
  "author": "kjirou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kjirou/unlimited-blog-works/issues"
  },
  "homepage": "https://github.com/kjirou/unlimited-blog-works#readme",
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "files": [
    "LICENSE",
    "README.md",
    "bin",
    "dist",
    "package.json",
    "presets"
  ],
  "bin": {
    "ubw": "bin/cli.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^7.0.0",
    "@types/js-yaml": "^3.12.1",
    "@types/klaw-sync": "^6.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/sinon": "^7.0.13",
    "clear-module": "^3.2.0",
    "klaw-sync": "^6.0.0",
    "mocha": "^6.1.4",
    "serve": "^14.2.1",
    "sinon": "^7.3.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "date-fns-timezone": "^0.1.4",
    "feed": "^3.0.0",
    "fs-extra": "^8.0.1",
    "js-yaml": "^3.13.1",
    "minimist": "^1.2.8",
    "minimist-subcommand": "^3.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rehype-autolink-headings": "^2.0.5",
    "rehype-document": "^3.0.1",
    "rehype-format": "^2.3.2",
    "rehype-parse": "^6.0.0",
    "rehype-raw": "^4.0.1",
    "rehype-slug": "^2.0.3",
    "rehype-stringify": "^6.0.0",
    "remark-frontmatter": "^1.3.2",
    "remark-parse": "^6.0.3",
    "remark-rehype": "^4.0.1",
    "unified": "^7.1.0",
    "unist-util-remove": "^1.0.3",
    "unist-util-visit": "^1.4.1"
  }
}
